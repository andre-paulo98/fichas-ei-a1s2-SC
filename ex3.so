.data
CR: .word32 0x10000
DT: .word32 0x10008

A: .word 10
B: .word 8
C: .word 0

.code
main:

lwu r1,CR(r0) ; guardar em memoria o controlo
lwu r2,DT(r0) ; e dados

ld r4,A(r0) ; código para somar dois números
ld r5,B(r0)
dadd r3,r4,r5
sd r3,C(r0) ; guarda para a memória

sd r3,(r2) ; 

daddi r10,r0,2
sd r10,(r1)


halt


